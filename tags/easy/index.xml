&lt;code id="codeblock-id-0" style="display:none;">package easy
import (
&amp;#34;strconv&amp;#34;
)
func HJ5(str string) int {
res, err := strconv.ParseInt(str, 16, 32)
if err != nil {
panic(err)
}
return int(res)
